<view><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">紧急联系人</view></cu-custom><view class="cu-bar bg-white search fixed" style="{{'top:'+(CustomBar+'px')+';'}}"><view class="search-form round"><text class="cuIcon-search"></text><input type="text" placeholder="输入搜索的关键词" confirm-type="search"/></view><view class="action"><button class="cu-btn bg-gradual-green shadow-blur round">搜索</button></view></view><scroll-view class="indexes" style="{{'height:'+('calc(100vh - '+CustomBar+'px - 50px)')+';'}}" scroll-y scroll-into-view="{{'indexes-'+listCurID}}" scroll-with-animation="{{true}}" enable-back-to-top="{{true}}"><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['indexItem-'+item.$orig.name]}}" id="{{'indexes-'+item.$orig.name}}" data-index="{{item.$orig.name}}"><view class="padding">{{item.$orig.name}}</view><view class="cu-list menu-avatar no-padding"><block wx:for="{{2}}" wx:for-item="items" wx:for-index="sub" wx:key="sub"><view class="cu-item"><view class="cu-avatar round lg">{{item.g0}}</view><view class="content"><view class="text-grey">{{item.$orig.name}}<text class="text-abc">{{list[sub].name}}</text></view><view class="text-gray text-sm">{{'德玛西亚 '+(sub+2)+''}}</view></view></view></block></view></view></block></block></scroll-view></view>